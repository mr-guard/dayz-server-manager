{
    "name": "server-manager-ui",
    "version": "1.3.0",
    "scripts": {
        "build": "npm run ng -- build --prod --build-optimizer=true --statsJson=true && npm run build:version",
        "build:debug": "npm run ng -- build --prod --source-map --build-optimizer=true --statsJson=true && npm run build:version",
        "build:version": "node scripts/version.js",
        "bundle-report": "webpack-bundle-analyzer ../dist/ui/stats-es2015.json",
        "e2e": "npm run ng -- e2e",
        "generate:component": "./node_modules/.bin/ng generate @sbpro/ng:component",
        "generate:directive": "./node_modules/.bin/ng generate @sbpro/ng:directive",
        "generate:module": "./node_modules/.bin/ng generate @sbpro/ng:module",
        "generate:service": "./node_modules/.bin/ng generate @sbpro/ng:service",
        "lint:fix": "npm run ng -- lint --fix",
        "lint": "npm run ng -- lint",
        "ng": "npx cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node_modules/.bin/ng",
        "serve": "./node_modules/.bin/static-server ../dist/ui",
        "start": "node scripts/start.js",
        "test": "npm run ng -- test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.0",
        "@angular/cdk": "~11.0.0",
        "@angular/common": "~11.0.0",
        "@angular/compiler": "~11.0.0",
        "@angular/core": "~11.0.0",
        "@angular/forms": "~11.0.0",
        "@angular/material": "~11.0.0",
        "@angular/platform-browser": "~11.0.0",
        "@angular/platform-browser-dynamic": "~11.0.0",
        "@angular/router": "~11.0.0",
        "@asymmetrik/ngx-leaflet": "^8.1.0",
        "@fortawesome/angular-fontawesome": "0.8.0",
        "@fortawesome/fontawesome-svg-core": "1.2.32",
        "@fortawesome/free-brands-svg-icons": "5.15.1",
        "@fortawesome/free-regular-svg-icons": "5.15.1",
        "@fortawesome/free-solid-svg-icons": "5.15.1",
        "@ng-bootstrap/ng-bootstrap": "^9.0.0",
        "@ng-select/ng-select": "6.1.0",
        "ag-grid-angular": "25.3.0",
        "ag-grid-community": "25.3.0",
        "chart.js": "2.9.4",
        "leaflet": "1.7.1",
        "ngx-gauge": "3.0.0",
        "object-hash": "2.0.3",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3",
        "stream": "0.0.2",
        "timers": "0.1.1",
        "tslib": "2.0.3",
        "uuid": "8.3.1",
        "webpack-bundle-analyzer": "4.1.0",
        "xml2js": "0.4.23",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1100.2",
        "@angular/cli": "~11.0.0",
        "@angular/compiler-cli": "~11.0.0",
        "@angular/language-service": "~11.0.0",
        "@angular/localize": "~11.0.0",
        "@inip/static-server": "1.0.1",
        "@sbpro/ng": "1.4.2",
        "@types/chart.js": "2.9.28",
        "@types/jasmine": "3.6.1",
        "@types/jasminewd2": "2.0.8",
        "@types/leaflet": "^1.7.0",
        "@types/node": "14.14.8",
        "@types/object-hash": "1.3.4",
        "@types/uuid": "8.3.0",
        "@types/xml2js": "0.4.8",
        "bootstrap": "4.5.3",
        "chokidar": "3.4.3",
        "codelyzer": "6.0.1",
        "concurrently": "5.3.0",
        "cross-env": "7.0.2",
        "jasmine-core": "3.6.0",
        "jasmine-spec-reporter": "6.0.0",
        "karma": "5.1.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "4.0.1",
        "karma-jasmine-html-reporter": "1.5.4",
        "prettier": "2.1.2",
        "protractor": "7.0.0",
        "shelljs": "0.8.4",
        "ts-node": "9.0.0",
        "tslint": "6.1.3",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "4.0.5"
    }
}
